{
  "name": "testonwheels",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "client",
    "server",
    "database"
  ],
  "scripts": {
    "dev:server": "cd server && pnpm start:dev",
    "dev:client": "cd client && pnpm dev",
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\"",
    "lint": "pnpm -r lint",
    "test:ts": "pnpm -r test:ts",
    "test": "pnpm -r test",
    "db:reset": "cd db && pnpm db:reset",
    "db:seed": "cd db && pnpm db:seed",
    "db:studio": "cd db && pnpm db:studio",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "enndy",
  "license": "ISC",
  "devDependencies": {
    "drizzle-orm": "0.40.1",
    "drizzle-kit": "0.30.0",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "overrides": {
      "drizzle-orm": "0.40.1"
    }
  }
}
